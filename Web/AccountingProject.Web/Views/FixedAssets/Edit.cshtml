@model AccountingProject.Web.ViewModels.FixedAssets.EditFixedAssetInputModel
@{
    this.ViewData["Title"] = "Edit a fixed asset";
}

<h2 class="mx-auto w-50">@this.ViewData["Title"]</h2>

<form class="mx-auto w-50" method="post" id="editForm">    
    <partial name="_FixedAssetInputViewPartial" model="Model" />    
    <hr />
    <input type="hidden" asp-for="Id" />
    <div class="d-flex justify-content-center">
        <button type="submit" class="btn btn-block btn-primary text-white mb-3">Save changes</button>
    </div>
</form>


